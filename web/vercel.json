{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "proof-of-personhood-web",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "production": true
    }
  },
  "github": {
    "enabled": true,
    "autoJobCancelation": true
  }
}
